{
  "manifest_version": 2,
  "name": "Accessible",
  "version": "0.1.0",
  "description": "A (work-in-progress) tool that enhance browsing the web.",
  "icons": {
    "48": "a.svg"
  },
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["content_scripts/dist/main.js"],
      "css": ["content_scripts/dist/main.css"]
    }
  ],
  "background": {
    "scripts": ["temp_experience_with_bg_scripts.js"]
  }
}
